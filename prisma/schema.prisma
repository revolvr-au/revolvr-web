model creator_profiles {
  user_id       String   @id @db.Uuid
  email         String   @unique
  status        String   @default("ACTIVE")
  display_name  String?
  handle        String?
  revenue_share Decimal  @default(0.45) @db.Decimal
  created_at    DateTime @default(now()) @db.Timestamptz(6)

  avatar_url String?
  bio        String? @db.Text

  @@index([user_id])
  @@schema("public")
}

