model live_polls {
  id         String       @id @default(dbgenerated("gen_random_uuid()")) @db.Uuid
  live_id    String       @unique(map: "live_polls_live_unique") @db.Uuid
  option_a   String
  option_b   String
  expires_at DateTime     @db.Timestamptz(6)
  created_at DateTime     @default(now()) @db.Timestamptz(6)
  live_votes live_votes[]

  @@schema("public")
}

/// This table contains check constraints and requires additional setup for migrations. Visit https://pris.ly/d/check-constraints for more info.
model live_votes {
  id         String     @id @default(dbgenerated("gen_random_uuid()")) @db.Uuid
  poll_id    String     @db.Uuid
  user_id    String     @db.Uuid
  choice     String
  created_at DateTime   @default(now()) @db.Timestamptz(6)
  live_polls live_polls @relation(fields: [poll_id], references: [id], onDelete: Cascade, onUpdate: NoAction)

  @@index([poll_id], map: "live_votes_poll_idx")
  @@index([user_id], map: "live_votes_user_idx")
  @@schema("public")
}

