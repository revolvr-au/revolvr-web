{
  "name": "revolvr-web",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "vercel-build": "npx prisma generate && next build --webpack",
    "dev": "next dev -p 3000",
    "migrate:deploy": "prisma migrate deploy",
    "build": "prisma generate && cross-env NODE_OPTIONS=--max_old_space_size=4096 next build --webpack",
    "build:ci": "prisma generate && cross-env NODE_OPTIONS=--max_old_space_size=4096 next build --webpack",
    "start": "next start",
    "lint": "eslint \"src/**/*.{js,jsx,ts,tsx}\"",
    "lint:fix": "eslint \"src/**/*.{js,jsx,ts,tsx}\" --fix",
    "lint:ci": "eslint \"src/**/*.{js,jsx,ts,tsx}\" --max-warnings=0",
    "postinstall": "prisma generate"
  },
  "dependencies": {
    "@livekit/components-react": "^2.9.20",
    "@livekit/components-styles": "^1.2.0",
    "@prisma/client": "^6.19.0",
    "@supabase/auth-helpers-nextjs": "^0.15.0",
    "@supabase/ssr": "^0.7.0",
    "@supabase/supabase-js": "^2.89.0",
    "framer-motion": "^12.34.3",
    "livekit-client": "^2.17.2",
    "livekit-server-sdk": "^2.15.0",
    "lottie-react": "^2.4.1",
    "lucide-react": "^0.574.0",
    "next": "^16.0.7",
    "prisma": "^6.19.0",
    "react": "19.2.0",
    "react-dom": "19.2.0",
    "stripe": "^20.1.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.2",
    "@next/eslint-plugin-next": "^16.1.1",
    "@tailwindcss/postcss": "^4.1.17",
    "@types/node": "^20",
    "@types/pg": "^8.16.0",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "baseline-browser-mapping": "^2.9.19",
    "cross-env": "^10.1.0",
    "cross-env-shell": "^7.0.3",
    "eslint": "^9.39.2",
    "eslint-config-next": "16.0.3",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-react-hooks": "^7.0.1",
    "postcss": "^8.5.6",
    "tailwindcss": "^4.1.17",
    "typescript": "^5",
    "typescript-eslint": "^8.51.0",
    "vercel": "^50.1.3"
  },
  "engines": {
    "node": ">=22 <23"
  }
}
